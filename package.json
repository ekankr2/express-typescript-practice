    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@sentry/node": "^5.15.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
